<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>shengmin-me</application>
  <version>1</version>
  <static-files>
    <include path="/**.png" expiration="30d" />
    <include path="/**.jpg" expiration="30d" />
    <include path="/**.gif" expiration="30d" />
    <include path="/**.js" expiration="1d" />
    <include path="/**.css" expiration="1d" />
    <include path="/favicon.ico" expiration="30d" />
    <include path="/**.htm" expiration="1d" />
    <include path="/**.html" expiration="30d" />
  </static-files>
  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>
  <threadsafe>true</threadsafe>
 
</appengine-web-app>
